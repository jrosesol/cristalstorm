	<g:HTMLPanel>
		<body>
            
			<div id="wrapper">

				<!-- header start -->
				<div id="header">
					<p>
						<g:SimplePanel ui:field="panelSlogan"></g:SimplePanel>			
					</p>
					<ul>
						<li class="no_bg">
							<a href="#Settings"><g:Label ui:field="lblSettings"></g:Label></a>
						</li>
						<li>
							<a href="#Logout"><g:Label ui:field="lblLogout"></g:Label></a>
